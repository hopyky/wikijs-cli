{
  "name": "wikijs-cli",
  "version": "1.4.0",
  "description": "A comprehensive command-line interface for Wiki.js",
  "type": "module",
  "main": "bin/wikijs",
  "bin": {
    "wikijs": "./bin/wikijs"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "link": "npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openclaw/wikijs-cli.git"
  },
  "homepage": "https://github.com/openclaw/wikijs-cli#readme",
  "bugs": {
    "url": "https://github.com/openclaw/wikijs-cli/issues"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "form-data": "^4.0.0",
    "inquirer": "^9.2.0",
    "ora": "^7.0.0",
    "table": "^6.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "wikijs",
    "wiki",
    "cli",
    "wiki.js",
    "graphql",
    "markdown",
    "documentation",
    "knowledge-base"
  ],
  "author": "OpenClaw",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "testMatch": ["**/tests/**/*.test.js"],
    "collectCoverageFrom": ["lib/**/*.js"],
    "coverageDirectory": "coverage",
    "moduleFileExtensions": ["js", "mjs"]
  }
}
